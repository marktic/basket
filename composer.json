{
  "name": "marktic/basket",
  "description": " A base for shoping carts ",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/marktic/basket",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "bytic/form": "^1.0 || ^2.0",
    "bytic/migrations": "~0.12",
    "bytic/orm": "^1.0 || ^2.0",
    "bytic/orm-behaviors": "^1.0 || ^2.0",
    "bytic/orm-filters": "^1.0 || ^2.0",
    "bytic/package-base": "^1.0",
    "bytic/translation": "^1.0 || ^2.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Marktic\\Basket\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marktic\\Basket\\Tests\\": "tests/src",
      "Marktic\\Basket\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
